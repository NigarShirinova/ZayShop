@using ZayShop.Entities
@model List<Brand>


<div class="carousel-item active">
    <div class="row">
        @foreach (var brand in Model)
        {
            <div class="col-3 p-md-5">
                <a href="#"><img class="img-fluid brand-img" src=@brand.Photo alt="Brand Logo"></a>
            </div>
        }
    </div>
</div>